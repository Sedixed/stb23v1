<?xml version="1.0" encoding="UTF-8"?>
<stbs xmlns:th="http://www.thymeleaf.org">
    <stb th:each="stb:${stbs}">
        <id th:utext="${stb.id}"></id>
        <title th:utext="${stb.title}"></title>
        <description th:utext="${stb.description}"></description>
        <validationDate th:utext="${stb.validationDate}"></validationDate>
        <clientName th:utext="${stb.clientName}"></clientName>
    </stb>
</stbs>